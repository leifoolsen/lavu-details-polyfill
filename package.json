{
  "name": "lavu-details-polyfill",
  "version": "0.0.0-semantically-released",
  "description": "This polyfill provides support for the <details> element across all modern browsers.",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "dev": "./node_modules/.bin/webpack --progress --colors --watch --mode=dev",
    "eslint": "./node_modules/.bin/eslint ./src",
    "jshint": "./node_modules/.bin/jshint ./src",
    "test": "./node_modules/.bin/mocha  -w --opts ./mocha.opts './test/**/*.spec.js'",
    "test:coverage": "./node_modules/.bin/babel-node ./node_modules/.bin/istanbul cover --root ./src ./node_modules/.bin/_mocha -- --opts ./mocha.opts  -r .istanbul-mocha.config.js './test/**/*.spec.js'",
    "pre-build": "npm run test:coverage && rm -rf lib",
    "build": "./node_modules/.bin/webpack --mode=build",
    "check-coverage": "./node_modules/.bin/istanbul check-coverage --statements 70 --branches 70 --functions 70 --lines 70",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run pre-build && npm run build && npm run check-coverage"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "czConfig": {
    "path": "./node_modules/cz-conventional-changelog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leifoolsen/lavu-details-polyfill.git"
  },
  "keywords": [
    "details",
    "polyfill"
  ],
  "author": "Leif Olsen (https://github.com/leifoolsen)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leifoolsen/lavu-details-polyfill/issues"
  },
  "homepage": "https://github.com/leifoolsen/lavu-details-polyfill#readme",
  "devDependencies": {
    "babel-cli": "6.6.4",
    "babel-core": "6.6.4",
    "babel-eslint": "5.0.0",
    "babel-loader": "6.2.4",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-polyfill": "6.6.1",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.6.0",
    "cz-conventional-changelog": "1.1.5",
    "eslint": "2.2.0",
    "eslint-loader": "1.3.0",
    "ghooks": "1.0.3",
    "gulp": "3.9.1",
    "gulp-git": "1.7.0",
    "ignore-styles": "1.2.0",
    "istanbul": "1.0.0-alpha.2",
    "jsdom": "8.1.0",
    "jsdomify": "1.1.1",
    "jshint": "2.9.1",
    "mocha": "2.4.5",
    "require-uncached": "1.0.2",
    "semantic-release": "^4.3.5",
    "sinon": "1.17.3",
    "webpack": "1.12.14",
    "yargs": "4.2.0"
  }
}
